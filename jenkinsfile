pipeline {
    agent {label 'CENTOSJAVA11'}
    stages {
        stage('gitclone') {
            steps {
                git url: "https://github.com/krishna5683akp/openmrs-standalone.git", branch: "PROJECT_1.1"
            }
        }
        stage('build') {
            steps {
                sh "mvn package"
            }
        }
    }
}